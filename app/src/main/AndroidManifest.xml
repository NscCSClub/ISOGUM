<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nsccsclub.isogum">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FunctionActivity"
            android:label="@string/function_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateFunctionActivity"
            android:label="@string/create_name"
            android:parentActivityName=".FunctionActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FunctionActivity" />
        </activity>
        <activity
            android:name=".CreateVariableActivity"
            android:label="@string/create_name"
            android:parentActivityName=".FunctionActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FunctionActivity" />
        </activity>
        <activity
            android:name=".VariableActivity"
            android:label="@string/title_activity_variable"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FunctionActivity" />
        </activity>
        <activity
            android:name=".HelpActivity"
            android:label="@string/create_name"
            android:parentActivityName=".FunctionActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FunctionActivity" />
        </activity>
        <activity android:name=".RunActivity">
            android:name=".RunActivity"
            android:label="@string/run_name"
            android:parentActivityName=".FunctionActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FunctionActivity" />

        </activity>

        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />


    </application>

</manifest>
